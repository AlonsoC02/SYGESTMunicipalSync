    <link href="~/Patentes/assets/vendor/icofont/icofont.min.css" rel="stylesheet">
    <link href="~/Patentes/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/Patentes/assets/vendor/venobox/venobox.css" rel="stylesheet">
    <link href="~/Patentes/assets/vendor/line-awesome/css/line-awesome.min.css" rel="stylesheet">
    <link href="~/Patentes/assets/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">


    <link href="~/css/style.css" rel="stylesheet" />
    <!-- Template Main CSS File -->
    <link href="~/Patentes/assets/css/style.css" rel="stylesheet">

    <link href="~/lib/syncfusion-javascript/Content/ej/web/bootstrap-theme/ej.web.all.min.css" rel="stylesheet" />
    <link href="~/lib/ej.responsive.css" rel="stylesheet" />
    <script src="~/js/ej.web.all.min.js"></script>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
          integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p"
          crossorigin="anonymous" />
    <link href="~/css/Paginador.css" rel="stylesheet" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
</head>
<body>

    @* LOGIN -Registrarse *@
    <partial name="_Encabezado" />
    @* NAV *@
    <!-- container section start -->
    <header>
        <nav class="navbar navbar-expand-lg ftco_navbar ftco-navbar-light" id="ftco-navbar">
            <div class="container-fluid" style="padding-right: 25px">
                @* LOGO  *@
                <div class="logo-img">
                    <a asp-area="OFGA" asp-controller="Home" asp-action="Index">
                        <img src="~/images/LogoMuni2.png" alt="Oficina de Patentes">
                    </a>
                </div>
                @* LOGO  *@
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav"
                        aria-controls="ftco-nav"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="fa fa-bars"></span> Menu
                </button>

                <div class="collapse navbar-collapse" id="ftco-nav">

                    <ul class="navbar-nav m-auto">

                        @* LINK DE INICIO *@
                        <li class="nav-item">
                            <a class="nav-link" asp-area=""
                               asp-controller="Home" asp-action="Index">Inicio</a>
                        </li>

                        @* LINK DE INICIO OFPA *@
                        <li class="nav-item active">
                            <a class="nav-link" asp-area="PATENTES"
                               asp-controller="Home" asp-action="Index">Oficina de Patentes</a>
                        </li>

                        @* MENU DE MANTENIMIENTO PARA ADMINISTRADOR OFGA *@
                        @if (SYGESTMunicipalSync.Areas.Admin.Models.Utilitarios.MenuMantOFGA != "")
                        {
                            <li class="nav-item dropdown text-white-50">
                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropDownMenuLink"
                                   role="button" data-toggle="dropdown" aria-haspopup="true"
                                   aria-expanded="false">
                                    Mantenimiento
                                </a>
                                @* CRUD DE TIPOS CLASIFICACIÓN *@
                                <ul class="dropdown-menu" aria-labelledby="navabarDropDownMenuLink">
                                    @*@if (SYGESTMunicipalSync.Areas.Admin.Models.Utilitarios.ListaController.Contains("Clasificación Materiales"))
                    {*@
                                    <li class="nav-item active">
                                        <a class="nav-link" asp-area="PATENTES"
                                           asp-controller="Establecimiento" asp-action="Create"> Establecimiento</a>
                                    </li>

                                    @*}*@
                                    @* CRUD DE MATERIALES ADMITIDOS *@
                                    @*@if (SYGESTMunicipalSync.Areas.Admin.Models.Utilitarios.ListaController.Contains("Materiales Admitidos"))
                    {*@
                                    <li class="nav-item active">
                                        <a class="nav-link" asp-area="PATENTES" asp-controller="Home" asp-action="Index">Solicitud de Patentes</a>
                                    </li>
                                    @*}*@


                                </ul>
                            </li>
                        }
                        <li class="nav-item dropdown text-white-50">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropDownMenuLink"
                               role="button" data-toggle="dropdown" aria-haspopup="true"
                               aria-expanded="false">
                                Información
                            </a>



                            <div class="dropdown-menu" aria-labelledby="navabarDropDownMenuLink">

                                <a href="#inicio_Servicios" class="dropdown-item"> Pasos para solicitud de Patentes</a>

                                <a href="#requerimientos" class="dropdown-item"> Requerimientos para hacer solicitud</a>

                                <a href="#contacto_ubicacion" class="dropdown-item">Contacto y Ubicación</a>

                            </div>
                        </li>


                    </ul>
                </div>
            </div>
        </nav>

    </header>

    <div class="show fullscreen">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <!-- ======= Footer ======= -->
    <footer id="footer">
        <div class="footer-top">
            <div class="container">
                <div class="row">

                    <div class="col-lg-3 col-md-6 footer-info">
                        <h3>Municipalidad de Hojancha</h3>
                        <p>Siempre pensando en la comodidad para usted, ofrece sus servicios en línea y al alcalce de sus manos.</p>
                    </div>

                    <div class="col-lg-3 col-md-6 footer-links">

                        <h4>Servicios</h4>
                        <ul>

                            <li><a asp-area="PATENTES" asp-controller="Home" asp-action="Index" class="py-1 d-block"><span class="ion-ios-arrow-forward mr-3"></span>Oficina de Patentes</a></li>
                            <li><a asp-area="OFGA" asp-controller="Home" asp-action="Index" class="py-1 d-block"><span class="ion-ios-arrow-forward mr-3"></span>Oficina de Gestión Ambiental</a></li>
                            <li><a asp-area="OFIM" asp-controller="Home" asp-action="Index" class="py-1 d-block"><span class="ion-ios-arrow-forward mr-3"></span>Oficina de La Mujer</a></li>
                        </ul>
                    </div>

                    <div class="col-lg-3 col-md-6 footer-contact">
                        <h4>Información del contacto</h4>
                        <p>
                            <strong>Ubicación: </strong> Costado Norte del Parque<br>
                            Hojancha, Guanacaste, Costa Rica <br><br>
                            <strong>Teléfono:</strong> 26599116 Ext.223<br>
                            <strong>Email:</strong> info@munihojancha.com<br>
                        </p>

                        <div class="social-links">


                            <a href="https://www.facebook.com/MUNICIPALIDAD-DE-HOJANCHA-218380324851885/" class="facebook" target="_blank"><i class="icofont-facebook"></i></a>
                            <a href="https://www.instagram.com/explore/locations/256658138352395/municipalidad-hojancha/" class="instagram" target="_blank"><i class="icofont-instagram"></i></a>

                        </div>

                    </div>

                    <div class="col-lg-3 col-md-6 footer-newsletter">
                        <h4>Horario de Atención</h4>
                        <div class="opening-hours">
                            <h4>Abierto los Días:</h4>
                            <p class="ftco-footer-widget mb-4">
                                <span>Lunes – Viernes: 7:30 am a 3:30 pm</span>

                            </p>
                        </div>
                        @RenderBody()
                    </div>

                </div>
            </div>
        </div>
        @*sdfghjklkjhgfdsa*@
        <script src="js/jquery.min.js"></script>
        <script src="js/jquery-migrate-3.0.1.min.js"></script>
        <script src="js/jquery.easing.1.3.js"></script>
        <script src="js/jquery.waypoints.min.js"></script>
        <script src="js/jquery.stellar.min.js"></script>
        <script src="js/jquery.magnific-popup.min.js"></script>

        <div class="container">
            <div class="copyright">
                &copy; Copyright <strong><span>Municipalidad</span><span> Hojancha</span></strong>. Todo los Derechos Reservados
            </div>
            <div class="credits">
