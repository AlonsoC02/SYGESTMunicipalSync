                Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
            </div>
        </div>
    </footer><!-- End Footer -->

    <a href="#" class="back-to-top"><i class="icofont-simple-up"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/Patentes/assets/vendor/jquery/jquery.min.js"></script>
    <script src="~/Patentes/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Patentes/assets/vendor/jquery.easing/jquery.easing.min.js"></script>
    <script src="~/Patentes/assets/vendor/php-email-form/validate.js"></script>
    <script src="~/Patentes/assets/vendor/waypoints/jquery.waypoints.min.js"></script>
    <script src="~/Patentes/assets/vendor/counterup/counterup.min.js"></script>
    <script src="~/Patentes/assets/vendor/venobox/venobox.min.js"></script>
    <script src="~/Patentes/assets/vendor/owl.carousel/owl.carousel.min.js"></script>
    <script src="~/Patentes/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>


    <!-- Template Main JS File -->
    <script src="~/Patentes/assets/js/main.js"></script>
    <script src="~/js/jquery-ui.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/Paginador.js"></script>
    @RenderSection("Scripts", required: false)
